
<table class="table">
  <thead>
    <tr>
      <th>Category</th>
    </tr>
  </thead>

  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= link_to category.category, category %></td>
 		
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h4><%= link_to 'New Category', new_category_path(@category) %> </h4>



